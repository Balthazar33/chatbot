,WAAA,WAAA,GAAA,EAAA;AAEA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,IAAA,CAAA,UAAA,IAAA,EAAA,EAAA;AACA,QAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,UADA;AAEA,UAAA,WAAA,EAAA;AAFA,SAAA;AAKA,QAAA,KAAA,CACA,GADA,CAEA,oEAFA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,eAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA;AACA,iBAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,YAAA,EAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA;AACA,kBAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,UAAA,CAAA,CAAA,KAAA,IAAA,CAAA,UAAA,EAAA;AACA;AAEA,oBAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,GAAA,IAAA,SAAA,EAAA;AACA;AACA,kBAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,SAAA,CAAA,KAAA,CADA;AACA;AACA,oBAAA,WAAA,EAAA;AAFA,mBAAA;AAIA,kBAAA,IAAA,CAAA,WAAA,GAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,SAAA,CAAA,KAAA,CAAA;AACA,iBAPA,MAOA;AACA,sBAAA,SAAA,GAAA,IAAA,CAAA,KAAA,CACA,IAAA,CAAA,MAAA,KAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,SAAA,CAAA,MADA,CAAA,CADA,CAGA;;AACA,kBAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,SAAA,CAAA,SAAA,CADA;AAEA,oBAAA,WAAA,EAAA;AAFA,mBAAA;AAIA,kBAAA,IAAA,CAAA,WAAA,GAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,SAAA,CAAA,SAAA,CAAA;AACA;;AAEA,gBAAA,IAAA,CAAA,OAAA,GAAA,CAAA,CArBA,CAqBA;AACA,eAtBA,MAsBA;AACA;AACA,gBAAA,IAAA,CAAA,WAAA,IAAA,EAAA,GACA,IAAA,CAAA,OAAA,GAAA,CADA,GAEA,IAAA,CAAA,OAAA,GAAA,CAFA;AAGA;AACA;AACA;;AACA,UAAA,IAAA,CAAA,UAAA,GAAA,EAAA,CApCA,CAoCA;;AAEA,cAAA,IAAA,CAAA,OAAA,KAAA,CAAA,EAAA;AACA;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,cAAA,IAAA,EAAA,yBADA;AAEA,cAAA,WAAA,EAAA;AAFA,aAAA;AAIA,WA5CA,CA8CA;;;AACA,cAAA,UAAA,GAAA,QAAA,CAAA,cAAA,CAAA,eAAA,CAAA;AACA,UAAA,UAAA,CAAA,SAAA,GAAA,UAAA,CAAA,YAAA,GAAA,GAAA;AACA,SArDA;AAsDA;AACA,KArEA;AAsEA,IAAA,WAtEA,uBAsEA,KAtEA,EAsEA;AACA,WAAA,UAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CAAA,CADA,CACA;;AACA,WAAA,UAAA,GAAA,KAAA,CAFA,CAEA;;AACA,WAAA,YAAA,CAAA,KAAA;AACA,KA1EA;AA2EA,IAAA,UA3EA,wBA2EA;AACA,WAAA,UAAA,GAAA,IAAA,CADA,CACA;;AACA,WAAA,KAAA,GAAA,EAAA;AACA;AA9EA;AA1BA,CAAA","sourcesContent":["<template>\n  <div id=\"app_wrapper\">\n    <!-- MAIN APP WRAPPER -->\n    <transition name=\"fade\">\n    <div class=\"chatbot_container\" v-if=\"chat_bot_open\">\n      <div class=\"bg_top_orange\">\n        <div class=\"intro_txt_wrapper\">\n          <p class=\"bot_title txt-bg\">IRIS</p>\n          <p class=\"txt-md hello_txt text-bold\">Hello</p>\n          <img src=\"@/assets/images/hand_wave.png\" alt />\n          <p class=\"iris_intro\">\n            I am Iris, a Virtual Assistant\n            <br />How may I help you?\n          </p>\n        </div>\n      </div>\n\n      <!-- BOT ICON -->\n      <div class=\"central_robot ellipse\">\n        <img src=\"@/assets/images/Vector.svg\" alt />\n      </div>\n      <div class=\"central_robot_hidden ellipse\">\n        <img src=\"@/assets/images/Vector.svg\" alt />\n      </div>\n\n      <!-- FRONT CARD -->\n\n      <div class=\"front_bottom_card\" id=\"front_card\">\n\n        <div id=\"faqs\" v-if=\"hide_input==true\">\n          <div class=\"faqs_title text-bold\">\n            <p>\n              Frequently asked questions\n              <img\n                src=\"@/assets/images/question_mark.svg\"\n                alt=\"Question mark\"\n              />\n            </p>\n          </div>\n\n          <!-- FAQ items -->\n          <div class=\"faq_items\">\n            <div\n              class=\"faq_item\"\n              v-for=\"(item,index) in questions\"\n              :key=\"'item'+index\"\n              @click=\"faq_clicked(index)\"\n            >\n              <span class=\"bullet_mark\"></span>\n              <span>{{item}}</span>\n            </div>\n          </div>\n        </div>\n\n        <!-- CHATS SECTION -->\n        <div\n          id=\"chats_wrapper\"\n          :class=\"hide_input===false?'card_scroll':''\"\n          v-if=\"hide_input===false\"\n        >\n          <div class=\"chat_Container\">\n            <div class=\"chat clearfix\" v-for=\"(entry,index) in chats\" :key=\"'entry'+index\">\n              <!-- Check if user or bot, place accordingly -->\n              <span :class=\"entry.user_or_bot=='bot'? 'bot_bubble':'user_bubble'\">{{entry.text}}</span>\n            </div>\n          </div>\n        </div>\n\n        <!-- INPUT SECTION -->\n\n        <div class=\"input_box\">\n          <div class=\"start_conv\" @click=\"hide_input=false\" v-if=\"hide_input==true\">\n            <img src=\"@/assets/images/chat_icon.svg\" alt=\"Chat icon\" />\n            <p>Start a new conversation</p>\n          </div>\n\n          <!-- INPUT FIELD -->\n          <div id=\"input_bottom\" v-if=\"!hide_input\">\n            <input type=\"text\" name=\"chat_input\" v-model=\"user_input\" @keyup.enter=\"send_Clicked\" />\n            <a href=\"javascript:void(0);\" @click=\"send_Clicked\" class=\"send_btn\">\n              <i class=\"fa fa-paper-plane\"></i>\n            </a>\n\n            <!-- GO BACK BUTTON -->\n            <a href=\"javascript:void(0);\" class=\"back_link\" @click=\"reset_chat\">\n              <i class=\"fa fa-backward\"></i>\n            </a>\n          </div>\n        </div>\n      </div>\n    </div>\n    </transition>\n    <div class=\"open_chatbot ellipse\" @click=\"open_chatbot\">\n        <img src=\"@/assets/images/Vector.svg\" alt />\n      </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport myMixin from '@/mixins/mixinexample.js'\nexport default {\n  name: \"Bot\",\n  mixins:[myMixin],\n  props: {\n    msg: String,\n  },\n  data() {\n    return {\n      questions: [\n        \"Can I redeem my FB before the original term?\",\n        \"How do I pay my Credit card bill?\",\n        \"How can I get my Account Statement?\",\n        \"What is the tenure of Fixed Deposit?\",\n      ],\n      user_input: \"\",\n      hide_input: true,\n      data: [],\n      message:\"Bot message\",\n      botReply: [],\n      user_entry: [],\n      nomatch: 0,\n      chats: [],\n      match_found: \"\",\n      chat_bot_open:false,\n    };\n  },\n  methods: {\n    open_chatbot(){\n      this.chat_bot_open = !this.chat_bot_open;\n    },\n    send_Clicked(index) {\n      this.match_found = \"\";\n\n      var self = this;\n      if (self.user_input != \"\") {\n        self.chats.push({\n          text: self.user_input,\n          user_or_bot: \"user\",\n        });\n\n        axios\n          .get(\n            \"https://my-json-server.typicode.com/Balthazar33/myjson_json/inputs\"\n          )\n          .then((response) => {\n            this.data = response.data;\n            for (var i = 0; i < self.data.length; i++) {\n              //loop over received response items\n              for (var j = 0; j < self.data[i][\"user_input\"].length; j++) {\n                //loop over user_input array for each\n                if (self.data[i].user_input[j] == self.user_input) {\n                  //Match is found\n\n                  if (self.data[i].tag == \"finance\") {\n                    //If it is a question of finance category...\n                    self.chats.push({\n                      text: self.data[i].responses[index], //...provide accurate response\n                      user_or_bot: \"bot\",\n                    });\n                    self.match_found = self.data[i].responses[index];\n                  } else {\n                    var randomint = Math.floor(\n                      Math.random() * self.data[i].responses.length\n                    ); //random integer from 0 to length of response array\n                    self.chats.push({\n                      text: self.data[i].responses[randomint],\n                      user_or_bot: \"bot\",\n                    });\n                    self.match_found = self.data[i].responses[randomint];\n                  }\n\n                  self.nomatch = 0; //Update match flag\n                } else {\n                  //If match is not found, set matchfound flag\n                  self.match_found == \"\"\n                    ? (self.nomatch = 1)\n                    : (self.nomatch = 0);\n                }\n              }\n            }\n            self.user_input = \"\"; //Clear input field\n\n            if (self.nomatch === 1) {\n              //If no match is found, print default\n              self.chats.push({\n                text: \"Sorry, didn't get that.\",\n                user_or_bot: \"bot\",\n              });\n            }\n\n            //Make div scroll to top\n            var front_card = document.getElementById(\"chats_wrapper\");\n            front_card.scrollTop = front_card.scrollHeight + 200;\n          });\n      }\n    },\n    faq_clicked(index) {\n      this.user_input = this.questions[index]; //Use preset faq question for chat\n      this.hide_input = false; //Make faqs invisible\n      this.send_Clicked(index);\n    },\n    reset_chat() {\n      this.hide_input = true; //Go back to start page\n      this.chats = [];\n    },\n  },\n};\n</script>\n\n<style scoped>\n@import \"../assets/style.css\";\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity 0.5s;\n}\n.fade-enter, .fade-leave-to {\n  opacity:0;\n}\n</style>"],"sourceRoot":"src/components"}]}